{
  "name": "hexa-worker",
  "compatibility_date": "2025-08-16",
  "assets": {
    "directory": "./dist"
  },
  "main": "src/worker-voice.ts",
  "durable_objects": {
    "bindings": [
      {
        "name": "VOICE_SESSION",
        "class_name": "VoiceSession"
      }
    ]
  },
  "env": {
    "OPENAI_REALTIME_MODEL": "your-openai-api-key-here"
  },
  "build": {
    "command": "npm run build:worker",
    "watch_dir": "src"
  }
}
