{
  "name": "hexa-worker",
  "compatibility_date": "2025-08-16",
  "assets": {
    "directory": "./dist"
  },
  "main": "src/worker-voice/index.ts",
  "durable_objects": {
    "bindings": [
      {
        "name": "VOICE_SESSION",
        "class_name": "VoiceSession"
      }
    ]
  },

  "migrations": [
    {
      "tag": "v1",
      "new_classes": [
        "VoiceSession"
      ]
    }
  ],

  "build": {
    "command": "yarn build:worker",
    "watch_dir": "src"
  },

  "observability": {
    "logs": {
      "enabled": true
    }
  }
}
